{
  "name": "wdio-jasmine-boilerplate",
  "version": "1.0.0",
  "description": "WebdriverIO with Jasmine and expect-webdriverio",
  "scripts": {
    "compile": "tsc --incremental",
    "lint": "tslint --project tsconfig.json --fix",
    "test": "wdio wdio.CHROME.conf.js",
    "test:headless": "CHROME_HEADLESS=true npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgrybyk/wdio-jasmine-boilerplate.git"
  },
  "keywords": [
    "webdriverio",
    "devtools",
    "expect-webdriverio"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mgrybyk/wdio-jasmine-boilerplate/issues"
  },
  "homepage": "https://github.com/mgrybyk/wdio-jasmine-boilerplate#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all --parallel compile lint"
    }
  },
  "devDependencies": {
    "husky": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@types/jasmine": "^3.5.0",
    "@wdio/cli": "^5.18.1",
    "@wdio/jasmine-framework": "^5.16.15",
    "@wdio/local-runner": "^5.18.0",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.16.11",
    "@wdio/sync": "^5.17.0",
    "expect": "^24.9.0",
    "expect-webdriverio": "^0.1.3-beta.7"
  },
  "bundledDependencies": []
}
